.codeImage_mask{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: black;
  opacity: 0.7;
  z-index: 90;
}
.codeImage{
  position:absolute;
  top:10%;
  left: 5%;
  width: 90%;
  height: 80%;
  background-color: white;
  border-radius: 15px;
  z-index: 100;
}
.image_title{
  border-left: 3px solid #1358B5;
  width: 90%;
  height: 21px;
  font-size: 15px;
  margin: 15px 0px 15px 15px;
}
.image_title text{
  margin: 0 10px;
  font-weight: bold;
}
.image_number{
  width: 85%;
  height: 57%;
  margin: 0 auto;
}
.image_limit{
  display: block;
  text-align: center;
  color: #1a75ff;
  font-size: 14px;
  margin: 6px 0;
}
.image_buttom{
  width: 80%;
  height: 10%;
  background: #1a75ff;
  border-radius: 25px;
  margin: 0 auto;
  display: flex;
  justify-content: center;
}
.image_buttom text{
  display: table-cell;
  text-align: center;
  color: white;
  vertical-align:middle;
}
.image_test{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: white;
}
.image_choose{
  width: calc(96% / 3);
  height: calc(96% / 3);
  border-radius: 10px;
  float: left;
}
.image_choose_left{
  margin: 0 2% 2% 0;
}
.image_choose_right{
  margin: 0 0 2% 0;
}
.image_choose_flie{
  width: calc((96% - 12px) / 3);
  height: calc((96% - 12px) / 3);
  margin: 0 2% 2% 0;
  border-radius: 10px;
  float: left;
}
.image_preview{
  position:absolute;
  top:10%;
  left: 5%;
  width: 90%;
  height: 80%;
  background-color: white;
  border-radius: 15px;
  z-index: 110;
}