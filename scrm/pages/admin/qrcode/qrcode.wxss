.qrcode_mask{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: black;
  opacity: 0.7;
  z-index: 90;
}
.qrcode_domain{
  width: 100%;
  height: 100%;
  z-index: 100;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.qrcode_domain text{
  display: block;
  text-align: center;
  color: white;
}
