<view class='qrcode_mask'></view>
<!-- bindtap="previewImg" hidden="{{canvasHidden}}" -->
<view class='qrcode_domain'>
  <text>二维码</text>
  <view class='qrcode_img' style='width: {{qrcodeW}}; height: {{qrcodeW}};'>
    <canvas  style="width: 100%;height: 100%;background:#f1f1f1;" canvas-id="mycanvas"/>
  </view>
  <view id='0' bindtap='qrcodeStatus' class='qrcode_img_off'>
    <import src="../../template/image.wxml"/>
    <template is="fork"></template>
  </view>
</view>
